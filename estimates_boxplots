estimates <- read.csv ("testagrade6062.csv" )
View (estimates)
par (mfrow= c(1,1))

with (estimates, boxplot (estimate ~Design))
with (estimates, boxplot (SE_estimate ~ Design))
with (estimates, boxplot (estimate ~Days))
with (estimates, boxplot (SE_estimate ~ Days))
abline (h = c (0.0000267), col = "red", lty = 2)
abline (h = c (0.0000173), col = "red", lty = 2)
with (estimates, boxplot (estimate ~Estacoes))
with (estimates, boxplot (SE_estimate ~Estacoes))
with (estimates, boxplot (estimate ~Dias))
